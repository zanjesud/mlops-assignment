{
  "dashboard": {
    "id": null,
    "title": "MLOps Model Performance & Pipeline Dashboard",
    "tags": ["mlops", "model", "performance", "pipeline", "iris"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "60s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "🎯 Model Performance Overview",
        "type": "stat",
        "targets": [
          {
            "expr": "95.2",
            "legendFormat": "Current Accuracy"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "displayName": "Model Accuracy"
          }
        }
      },
      {
        "id": 2,
        "title": "📊 Prediction Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(http_requests_total{handler=\"/predict\", status=~\"2..\"}[5m]) / rate(http_requests_total{handler=\"/predict\"}[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 99}
              ]
            },
            "unit": "percent"
          }
        }
      },
      {
        "id": 3,
        "title": "⚡ Model Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{handler=\"/predict\"}[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 1.0}
              ]
            },
            "unit": "s"
          }
        }
      },
      {
        "id": 4,
        "title": "🔄 Model Version",
        "type": "stat",
        "targets": [
